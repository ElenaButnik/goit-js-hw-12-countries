<div class="country">
    <h1 class="country__name">{{name}}</h1>
    <p class="country__capital">Capital: {{capital}}</p>
    <p class="country__population">Population: {{population}}</p>
    <p class="country__language">Languages: {{languages.name}}</p>
    {{#each languages}}
    <p> {{languages.name}}</p>
    {{/each}}
    <img class="country__flag" src ="{{flag}}" alt="{{name}}"></img>
</div>
